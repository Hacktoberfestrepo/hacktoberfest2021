#include <iostream>
using namespace std;

int main() 
{
    int n, reverse = 0, remainder;

    cout << "Enter any integer that you want to reverse: ";
    cin >> n;

    while(n != 0) {
        remainder = n%10;
        reverse = reverse*10 + remainder;
        n /= 10;
    }

    cout << "The reverse of the given number is: " << reverse;

    return 0;
}
